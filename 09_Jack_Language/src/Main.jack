// Save those locations and having a moving arrow
// 48 x 50 

class Main {
    function void main() {
        var Game game;
        let game = Game.new();
        do game.play();
        do game.dispose();

        // After quitting game
        do Screen.clearScreen();
        do Graphics.draw_end(1612);

        return;
    }
}

